{
    "name": "PoE Manager Backend Server",
    "script": "dist/app/app.js",
    "instances": "max",
    "exec_mode": "cluster",
    "autorestart": true,
    "watch": false,
    "max_memory_restart": "1G",
    "error_file": "logs/error.log",
    "out_file": "logs/out.log",
    "env": {
        "NODE_ENV": "development"
    },
    "env_production": {
        "NODE_ENV": "production"
    }
}